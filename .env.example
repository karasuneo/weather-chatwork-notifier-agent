# Environment variables for the application
AWS_PROFILE=

COMPLETION_MODEL=

# OpenWeatherMap API Key for weather data
# Get your API key from: https://openweathermap.org/api
OPENWEATHERMAP_API_KEY=your_openweathermap_api_key_here

# ChatWork API Token
# Get your API token from ChatWork: Settings -> API Settings
CHATWORK_API_TOKEN=your_chatwork_api_token_here

# ChatWork Room ID to send notifications to
# You can find room ID in the URL: https://www.chatwork.com/#!rid[ROOM_ID]
CHATWORK_ROOM_ID=your_chatwork_room_id_here